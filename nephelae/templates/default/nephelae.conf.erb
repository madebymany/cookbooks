check process nephelae
  with pidfile <%= @piddir %>/nephelae.pid
  start program = "nephelae start --piddir <%= @piddir%> --config <%= @conffile %> --logdir <%= @logdir %> --loglevel <%= @loglevel%>"
  stop program = "nephelae stop --piddir <%= @piddir%> --config <%= @conffile %> --logdir <%= @logdir %> --loglevel <%= @loglevel%>"
