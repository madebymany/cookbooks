check process nephelae
  with pidfile <%= @piddir %>/nephelae.pid
  start program = "/usr/bin/nephelae start --piddir <%= @piddir%> --config <%= @conffile %> --logdir <%= @logdir %> --loglevel <%= @loglevel%>"
  stop program = "/usr/bin/nephelae stop --piddir <%= @piddir%> --config <%= @conffile %> --logdir <%= @logdir %> --loglevel <%= @loglevel%>"
